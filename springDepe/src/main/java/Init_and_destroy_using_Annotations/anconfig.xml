<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xmlns:context="http://www.springframework.org/schema/context" 
 xmlns:p="http://www.springframework.org/schema/p" 
 xmlns:c="http://www.springframework.org/schema/c" 
 xsi:schemaLocation="http://www.springframework.org/schema/beans
                    http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
                    http://www.springframework.org/schema/context 
                    http://www.springframework.org/schema/context/spring-context-4.0.xsd">
   <context:annotation-config />                 
  <bean class="Init_and_destroy_using_Annotations.Address" name="address" p:city="Barauni" p:state="Bihar" />
  <bean class="Init_and_destroy_using_Annotations.Student" name="student" p:id="1" p:name="Prince kumar" p:address-ref="address" />
                   
</beans>